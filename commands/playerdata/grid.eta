<html>
<head>
<title></title>
</head>
    <style>
        .grid {
            border-collapse:collapse;
            width:680px;
        }
        .grid > tr:nth-child(even){
            padding:5px;
            border:1px solid #6F6F6F;
            font-family:Helvetica;
            font-size:1em;
            text-align:left;
            color:#F0F0F0;
            width:200px;
        }

        .grid > tr:nth-child(odd){
            padding:5px;
            border:1px solid #6F6F6F;
            font-family:Helvetica;
            font-size:1em;
            text-align:left;
            color:#F0F0F0;
            width:140px;
        }
        .title {
            width:100%;
            text-align:center;
            border-bottom:1px solid #6F6F6F;
            line-height:0.1em;
            margin:10px 0 20px;
            color:#F0F0F0;
        }
    </style>
    <body style="background-color:#4F4F4F">
        <hr style="height:5px; visibility:hidden;margin:0px;border:0px;">
        <h2 class="title">
            <span style="background-color:#4F4F4F;padding:0 10px;font-family:Helvetica;">Data</span>
        </h2>
        <p style="font-family:Helvetica;font-size:1em;color:#F0F0F0">Player data requested by: 420204969027698688 </p>
        <p style="font-family:Helvetica;font-size:1em;color:#F0F0F0">Username: tovernaar123 </p>
        <table class="grid">
        <% for (let row of it.table) { %>
            <tr>

            <% for (let entry of row) { %>
                <td><%= entry %></td>
            <%}%>

            </tr>
        <% } %>

        </table>
    </body>
</html>