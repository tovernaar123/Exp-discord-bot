<html>
<head>
<title></title>
</head>
    <style>
        .grid {
            border-collapse:collapse;
            width:680px;
        }
        td {
            padding:5px !important;
            border:1px solid #6F6F6F !important;
            font-family:Helvetica;
            font-size:1em;
            text-align:left;
            color:#F0F0F0;
            width:140px;
        }
        .title {
            width:100%;
            text-align:center;
            border-bottom:1px solid #6F6F6F;
            line-height:0.1em;
            margin:10px 0 20px;
            color:#F0F0F0;
        }
    </style>
    <body style="background-color:#2f3136">
        <hr style="height:5px; visibility:hidden;margin:0px;border:0px;">
        <h2 class="title">
            <span style="background-color:#4F4F4F;padding:0 10px;font-family:Helvetica;">Data</span>
        </h2>
        <p style="font-family:Helvetica;font-size:1em;color:#F0F0F0">Player data requested by: 420204969027698688 </p>
        <p style="font-family:Helvetica;font-size:1em;color:#F0F0F0">Username: tovernaar123 </p>
        <table class="grid">
        <% let i = 0 %>
        <% for (let row of it.grid) { %>
            <tr>
            <% for (let entry of row) { %>
                <% if(i % 2 === 0) { %>
                    <td style="width:200px"><%= entry %></td>
                <%} else {%>
                    <td style="width:140px;"><%= entry %></td>
                <%}%>
                <% i = i + 1%>
                <% console.log(i) %>
            <%}%>

            </tr>
        <% } %>

        </table>
    </body>
</html>
